{
  "network_security": {
    "tls_encryption": true,
    "firewall_rules": true,
    "vpc_isolation": true
  },
  "application_security": {
    "input_validation": true,
    "sql_injection_protection": true,
    "xss_protection": true
  },
  "access_control": {
    "rbac_enabled": true,
    "mfa_required": true,
    "api_rate_limiting": true
  }
}